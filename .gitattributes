# Auto detect text files and perform LF normalization
* text=auto

# Dart files
*.dart text eol=lf

# Flutter/Dart/Pub related
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.md text eol=lf
*.html text eol=lf
*.css text eol=lf
*.js text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.woff binary
*.woff2 binary
*.ttf binary
*.otf binary
*.eot binary
*.pdf binary
*.zip binary
*.jar binary
*.so binary
*.dylib binary
*.dll binary
*.a binary
*.lib binary
*.wasm binary

# Exclude files from exporting
.gitattributes export-ignore
.gitignore export-ignore
.github export-ignore 